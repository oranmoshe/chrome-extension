{
  "manifest_version": 2,

  "name": "Modify CSS and HTML",
  "version": "0.1.0",
  "description": "Lateral blog post demo extension",
  "web_accessible_resources": [
	  "words.json"
  ],
  "background": {
        "scripts": [
            "jquery.min.js","replaceText.js","background.js"
        ]
  },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery.min.js","replaceText.js","content.js"],
    "matches": ["<all_urls>"]
  }]

}
